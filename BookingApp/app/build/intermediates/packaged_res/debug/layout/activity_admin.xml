<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <Button
        android:id="@+id/btn_deleteAccount"
        android:layout_width="170dp"
        android:layout_height="54dp"
        android:text="@string/deleteAccount"
        tools:layout_editor_absoluteX="21dp"
        tools:layout_editor_absoluteY="131dp" />

    <Button
        android:id="@+id/logout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:text="Click here to log out"
        tools:layout_editor_absoluteX="29dp"
        tools:layout_editor_absoluteY="647dp" />

    <EditText
        android:id="@+id/editTextTextPersonName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="@string/AccountName"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="21dp"
        tools:layout_editor_absoluteY="77dp" />

    <EditText
        android:id="@+id/editTextTextPersonName2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="@string/CourseName"
        tools:layout_editor_absoluteX="21dp"
        tools:layout_editor_absoluteY="289dp" />

    <EditText
        android:id="@+id/editTextTextPersonName3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="@string/CourseCode"
        tools:layout_editor_absoluteX="21dp"
        tools:layout_editor_absoluteY="334dp" />

    <Button
        android:id="@+id/btn_deleteCourse"
        android:layout_width="173dp"
        android:layout_height="53dp"
        android:text="@string/deleteCourse"
        tools:layout_editor_absoluteX="21dp"
        tools:layout_editor_absoluteY="386dp" />

    <Button
        android:id="@+id/button"
        android:layout_width="173dp"
        android:layout_height="53dp"
        android:text="@string/EditByName"
        tools:layout_editor_absoluteX="21dp"
        tools:layout_editor_absoluteY="454dp" />

    <Button
        android:id="@+id/button2"
        android:layout_width="173dp"
        android:layout_height="53dp"
        android:text="@string/EditByCode"
        tools:layout_editor_absoluteX="225dp"
        tools:layout_editor_absoluteY="454dp" />

    <Button
        android:id="@+id/btn_createCourse"
        android:layout_width="173dp"
        android:layout_height="53dp"
        android:text="@string/create"
        tools:layout_editor_absoluteX="225dp"
        tools:layout_editor_absoluteY="386dp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/CourseManage"
        tools:layout_editor_absoluteX="21dp"
        tools:layout_editor_absoluteY="259dp"
        android:textSize="20dp"
        android:textColor="@color/black"
        android:textStyle="bold"/>

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/AccountManage"
        android:textColor="@color/black"
        android:textSize="20dp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="21dp"
        tools:layout_editor_absoluteY="50dp" />


</androidx.constraintlayout.widget.ConstraintLayout>